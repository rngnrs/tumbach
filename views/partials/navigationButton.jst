{{? target == "top"}}
    <a class="navigationButton navigationButtonTop" title="{{=it.tr.toTopText}}"
       href="#top" style="display: none;"><i class="mdi mdi-36px mdi-arrow-up-bold-circle"></i>
    </a>
{{?? target == "bottom"}}
    <a class="navigationButton navigationButtonBottom" title="{{=it.tr.toBottomText}}"
       href="#bottom" style="display: none;"><i class="mdi mdi-36px mdi-arrow-down-bold-circle"></i>
    </a>
{{?}}
