<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
{{#def.head}}
<body>
{{~["top", "bottom"] :target}}
    {{#def.navigationButton}}
{{~}}
<a id="top"></a>
{{#def.customHeader}}
<div class="wrap" {{#def.baseData}}>
    {{#def.toolbar}}
    <div class="wrappie">
        <div id="customHeaderPlaceholder" style="display: none;"></div>
        <div id="wrapper">
            {{#def.banner}}
            <div class="theTitle">
                <h1>
                    <a class="ajax" href="/{{=it.site.pathPrefix}}{{=it.board.name}}">{{=it.board.title}}</a>
                </h1>
            </div>
            <div id="threads">
                {{~it.threads :thread}}
                <a class="ajax" href="/{{=it.site.pathPrefix}}{{=it.board.name}}/arch/{{=thread.number}}.html"
                   class="archivalThreadLink">{{=thread.number}}.html</a>
                {{~}}
            </div>
            <hr />
            {{#def.postingSpeed}}
        </div>
    </div>
    {{#def.customFooter}}
</div>
<a id="bottom"></a>
</body>
</html>
